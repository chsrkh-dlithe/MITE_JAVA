import java.util.Scanner;
public class ReverseNum
{   
    static int Reverse(int n)
    {   int rev=0;
        int r=n;
        while(n>0 || n<0)
        {
            rev=rev*10+(n%10);
            n/=10;
        }
        return rev;
    }
    public static void main (String[] args) 
    {
        Scanner s = new Scanner(System.in);
        
        int n=s.nextInt();
        System.out.println(Reverse(n));
        
    }  
    
}
